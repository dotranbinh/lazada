<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Top menu item -->
    <!--
    This menu item's purpose is to overwrite another one defined in
    the base module in order to set new groups.
    -->
    <menuitem name="Quản lý đấu giá"
        id="menu_bidding_management_root"
              groups = "group_bidding_manager"
        sequence="1"/>

    <menuitem name="Sản phẩm"
        id="menu_product_seller"
        parent= "menu_bidding_management_root"
              sequence="2"
              action = 'action_open_seller_seller'
        />

    <menuitem name="Seller"
        id="menu_seller_seller"
        parent= "menu_bidding_management_root"
              sequence="3"
              action = 'action_open_seller_seller'
        />

</odoo>
